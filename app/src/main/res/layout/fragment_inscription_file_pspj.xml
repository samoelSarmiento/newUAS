<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingTop="70dp"
    android:background="@color/uas_plomo_fondo"
    android:orientation="vertical">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="34dp"
        android:layout_weight="0.80">




        <ListView
            android:id="@+id/psp_lstItemsIncriptionFiles"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

        </ListView>


    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="34dp"
        android:layout_weight="0.20"
        android:gravity="center"
        android:orientation="vertical">

        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Mandar comentario"
            android:id="@+id/btEnviarComentariopspSobreinscripcion"
            android:layout_gravity="center_horizontal"
            android:enabled="true"
            android:clickable="false" />

        <Button
            android:layout_width="169dp"
            android:layout_height="wrap_content"
            android:text="Aceptar"
            android:id="@+id/btAceptarpspSobreinscripcion"
            android:layout_gravity="center_horizontal"
            android:enabled="true" />

    </LinearLayout>
</LinearLayout>
