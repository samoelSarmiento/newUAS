<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="uas.pe.edu.pucp.newuas">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.VIBRATE" />


    <application
        android:allowBackup="true"
        android:label="NewUAS"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".view.SplashScreen"
            android:configChanges="orientation|keyboardHidden"
            android:label="@string/app_name"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".view.LogInActivity"
            android:configChanges="orientation|keyboardHidden"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".view.NavigationDrawerAcreditacion"
            android:configChanges="orientation|keyboardHidden"
            android:label="@string/title_activity_navigation_drawer_acreditacion"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme">

        </activity>

        <activity
            android:name=".view.NavigationDrawerInvestigacion"
            android:configChanges="orientation|keyboardHidden"
            android:label="@string/title_activity_navigation_drawer_investigacion"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme">

        </activity>

        <activity
            android:name=".view.NavigationDrawerTutoria"
            android:configChanges="orientation|keyboardHidden"
            android:label="@string/title_activity_navigation_drawer_tutoria"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme">


        </activity>

        <activity
            android:name=".view.NavigationDrawerTutoriaTutor"
            android:configChanges="orientation|keyboardHidden"
            android:label="@string/title_activity_navigation_drawer_tutoria_tutor"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme">


        </activity>

        <activity
            android:name=".view.MainActivity"
            android:configChanges="orientation|keyboardHidden"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme">

        </activity>

        <activity
            android:name=".view.NavigationDrawerPSP"
            android:configChanges="orientation|keyboardHidden"
            android:label="Psp"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme">

        </activity>
    </application>

</manifest>